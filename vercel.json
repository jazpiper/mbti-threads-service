{
  "outputDirectory": "public",
  "buildCommand": "npm install",
  "devCommand": "node src/server.js",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    },
    {
      "source": "/privacy",
      "destination": "/public/privacy/index.html"
    },
    {
      "source": "/terms",
      "destination": "/public/terms/index.html"
    }
  ]
}
